    "version": "0.2.0",
    "configurations": [
    



        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "Python Debugger: LW5",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args" : ["-i", "S8", "-j", "S8H"]
        }
    ]
}